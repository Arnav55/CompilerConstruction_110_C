%{
#include "y.tab.h"
#include <stdlib.h>
%}

%%

[0-9]+          { yylval.num = atoi(yytext); return NUMBER; }

"+"             { return PLUS; }
"*"             { return MULT; }
"-"             { return MINUS; }
"/"             { return DIVIDE; }

[ \t]+          { }
"\n"            { return '\n'; }
.               { }

%%

int yywrap() { return 1; }
